    "types",
    "type check"
  ],
  "author": "Robert Biggs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/composi/merge/issues"
  },
  "homepage": "https://github.com/composi/merge#readme",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^4.19.1",
    "jest": "^23.6.0",
    "prettier": "^1.13.7"
  },
  "publishConfig": {
